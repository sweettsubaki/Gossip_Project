 <%= form_tag url_for(action: 'create'), method: "post" do %>
      <%= label_tag 'first_name' %>
      <%= text_field_tag 'first_name', @id.first_name %>
      <br /> <br />
      <%= label_tag 'last_name' %>
      <%= text_field_tag 'last_name', @id.last_name %>
      <br /> <br />
      <%= label_tag 'email' %>
      <%= text_field_tag 'email', @id.email %>
      <br /> <br />
      <%= label_tag 'age' %>
      <%= number_field_tag 'age', @id.age %>
      <br /> <br />
      <%= label_tag 'city_id' %>
      <%= text_field_tag 'city_id', @id.city_id %>
      <br /> <br />
      <%= label_tag 'password' %>
      <%= text_field_tag 'password', @id.password %>
      <br /> <br />
      <%= label_tag 'description' %>
      <%= text_area_tag 'description', @id.description %>
      <br /> <br />


      <%= submit_tag "Signup" %> 
    <% end %>
  